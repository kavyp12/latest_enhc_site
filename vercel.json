{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "buildCommand": "npm run build",
        "env": {
          "NEXT_IGNORE_ESLINT": "1"
        }
      }
    }
  ],
  "framework": "nextjs",
  "installCommand": "npm install",
  "buildCommand": "next build --no-lint",
  "outputDirectory": ".next"
}